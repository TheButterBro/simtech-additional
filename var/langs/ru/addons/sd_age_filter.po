msgctxt "Addons::name::sd_age_filter"
msgid "Simtech Developments: Age filter"
msgstr "Simtech Developments: Фильтрация возраста"

msgctxt "Addons::description::sd_age_filter"
msgid "Allow you to add and manage departments of your company."
msgstr "Позволяет установить ограничение по возрасту на просмотр магазина."

msgctxt "Languages::sd_age_filter.age_verification"
msgid "Age verification"
msgstr "Проверка возраста"

msgctxt "Languages::sd_age_filter.warning"
msgid "Attention! The content of the site is prohibited for viewing by persons under [setting_minimum_age] years of age."
msgstr "Внимание! Содержимое сайта запрещено для просмотра лицами младше [setting_minimum_age] лет."

msgctxt "Languages::sd_age_filter.enter_age"
msgid "Please, enter your age:"
msgstr "Пожалуйста, введите свой возраст:"

msgctxt "Languages::sd_age_filter.access_denied"
msgid "Access denied. Viewing the contents of the site is prohibited for persons under [setting_minimum_age] years of age."
msgstr "Доступ отклонен. Просмотр содержимого сайта запрещен для лиц младше [setting_minimum_age] лет."

msgctxt "SettingsOptions::sd_age_filter::minimal_age"
msgid "Minimum age for website visitors (number)"
msgstr "Минимальный возраст для посетителей сайта (число)"

